<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="nianhuibao.com.androidprojects">

    <!-- 声明权限 -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <!-- 震动的权限。4.4及以上不需要声明权限，但低于4.4 时需要声明权限 -->
    <uses-permission android:name="android.permission.VIBRATE"/>
    <!-- 闪光灯的权限 -->
    <uses-permission android:name="android.permission.FLASHLIGHT"/>

    <!-- 开启 Android WebView 的安全浏览模式 -->
    <meta-data
        android:name="android.webkit.WebView.EnableSafeBrowsing"
        android:value="true"/>

    <!-- 有关项目依赖包发生 Manifest Merge 冲突的详细解决方案 tools:replace="android:name, android:label, android:theme,android:allowBackup" -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".spannableString_imageSpan.SpannableStringAndImageSpanActivity"/>
        <activity android:name=".easy_swipe_menu.EasySwipeMenuLayoutActivity"/>
        <activity android:name=".recyclerview_list_anim.RecyclerViewListAnimActivity"/>
        <activity android:name=".fadeIn_textView.FadeInTextViewActivity"/>
        <activity android:name=".horizontal_progressBar.ProgressBarActivity"/>
        <activity android:name=".pay_psd_inputView.PayPsdViewActivity"/>
        <activity android:name=".recyclerview_ce_hua.RecyclerCeHuaActivity">
        </activity>
        <activity android:name=".rang_seekBar.RangeSeekBarActivity"/>
        <activity android:name=".sticky_decoration.view.StickyDecorationActivity"/>
        <activity android:name=".sticky_decoration.view.StickyRecyclerViewActivity"/>
        <activity android:name=".sticky_decoration.view.PowerfulStickyRecyclerViewActivity"/>
        <activity android:name=".sticky_decoration.view.BeautifulRecyclerViewActivity"/>
        <activity android:name=".titanic.TitanicTextViewActivity"/>
        <activity android:name=".super_textView.SuperTextViewActivity"/>
        <activity android:name=".super_textView.SecondActivity"/>
        <activity android:name=".circular_reveal_anima.SearchViewActivity"/>
        <activity android:name=".circle_reveal_loading.CircleRevealLoadingActivity"/>
        <activity android:name=".ultraviewpager.UPVDemoActivity"/>
        <activity android:name=".ultraviewpager.PagerActivity"/>
        <activity android:name=".tradition_animation.TraditionActivity"/>
        <activity android:name=".tradition_animation.blur.BlurActivity"/>
        <activity android:name=".tradition_animation.TweenedAnimationActivity"/>
        <activity android:name=".tradition_animation.FrameAnimationActivity"/>
        <activity android:name=".property_animation.PropertyActivity"/>
        <activity android:name=".property_animation.ValueAnimationActivity"/>
        <activity android:name=".property_animation.AliPayAnimActivity"/>
        <activity android:name=".property_animation.RevealAnimatorActivity"/>
        <activity android:name=".property_animation.ShopCarAddAnimActivity"/>
        <activity android:name=".property_animation.LayoutAnimationsActivity"/>
        <activity android:name=".property_animation.PropertyAnimationActivity"/>
        <activity android:name=".radar_scan_view.WeiBoRadarScanViewActivity"/>
        <activity android:name=".drag_ball_view.DragBallViewActivity">
        </activity>
    </application>

</manifest>