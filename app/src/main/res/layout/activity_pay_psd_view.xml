<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="nianhuibao.com.androidprojects.pay_psd_inputView.PayPsdViewActivity">

    <nianhuibao.com.androidprojects.pay_psd_inputView.PayPsdInputView
        android:id="@+id/password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_10"
        android:paddingEnd="15dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="@dimen/dp_10"
        app:circleColor="@color/colorPrimary"
        app:divideLineColor="@color/colorAccent"
        app:divideLineWidth="1dp"
        app:maxCount="6"
        app:paddingStart="15dp"
        app:psdType="weChat"
        app:radius="@dimen/dp_4"/>
</RelativeLayout>
